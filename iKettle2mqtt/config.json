{
  "name": "ikettle2mqtt-dev",
  "version": "0.0.4",
  "slug": "ikettle2mqtt",
  "description": "A bridge to use the smarter iKettle2 via MQTT",
  "startup": "application",
  "arch": [
    "amd64",
    "armhf",
    "i386",
    "armv7",
    "aarch64"
  ],
  "boot": "auto",
  "host_network": true,
  "options": {
    "kettleIP": "your_ip_here",
    "kettleName": "Electric Kettle",
    "kettleUniqueID": "electric_kettle_mqtt",
    "mqttHost": "localhost",
    "mqttUser": "",
    "mqttPassword": "",
    "discoveryPrefix": "homeassistant",
    "discoveryEnabled": true,
    "additionalTemperatures": [70,80,90,100]
  },
  "schema": {
    "kettleIP": "str",
    "kettleName": "str",
    "kettleUniqueID": "str",
    "mqttHost": "str",
    "mqttUser": "str?",
    "mqttPassword": "str?",
    "discoveryPrefix": "str?",
    "discoveryEnabled": "bool?",
    "additionalTemperatures": ["int(50,100)"]
  }
}
